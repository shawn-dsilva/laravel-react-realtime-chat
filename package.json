{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production && php artisan websockets:serve",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "server": "php artisan websockets:serve & php artisan serve ",
        "all": "concurrently \"npm run server\" \"npm run watch\"",
        "nobs": "concurrently \"npm run dev\" \"npm run server\""
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/preset-react": "^7.0.0",
        "axios": "^0.21.1",
        "bootstrap": "^4.0.0",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^7.0",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.5",
        "lodash": "^4.17.20",
        "popper.js": "^1.12",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0"
    },
    "dependencies": {
        "concurrently": "^5.3.0",
        "laravel-echo": "^1.8.0",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.33",
        "pusher-js": "^6.0.3",
        "react-easy-crop": "^3.3.1",
        "react-moment": "^1.1.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "reactstrap": "^8.4.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0"
    },
    "homepage": "https://demos.shawndsilva.com/realtime-chat-app"
}
